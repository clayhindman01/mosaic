{
  "expo": {
    "name": "mosaic",
    "slug": "mosaic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
      "expo-font",
      {
          "fonts": [
            "./assets/fonts/Tinos-Regular.ttf",
            "./assets/fonts/Tinos-Bold.ttf",
            "./assets/fonts/Tinos-Italic.ttf",
            "./assets/fonts/Tinos-BoldItalic.ttf"
          ],
          "ios": {
            "fonts": ["./assets/fonts/Tinos-Regular.ttf", "./assets/fonts/Tinos-Bold.ttf"]
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take photos to complete tiles"
        }
      ]
  ]
  }
}
